class Solution {
    public int[][] solution(int[][] arr1, int[][] arr2) {
        int[][] arrSum = new int[arr1.length][arr1[0].length];
        
        for(int i = 0; i < arrSum.length; i++){
            for(int j = 0; j < arrSum[0].length; j++){
                arrSum[i][j] = arr1[i][j] + arr2[i][j];
            }
        }
        
        return arrSum;
    }
}
